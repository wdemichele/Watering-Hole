<div class="databox primary" style="border-radius:25px;">
    <h2>Add My Tags</h2>

    {{#each user.tags}}
        <div name="chip" class="chip" onclick="checked(this)">
            {{this}}
            <input style="display: none;" type="checkbox" id="{{this}}" name="{{this}}" value="{{this}}">
        </div>
    {{/each}}

    <br><br>
    
    <button class="tertiary right bar-btn" type="submit" id="tags_button" name="tags_button">Set Tags</button>
        
  </div>